      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/isos/:id",
      "handler": "iso.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/isos/:id",
      "handler": "iso.delete",
      "config": {
        "policies": []
      }
    }
  ]
