{
  "kind": "collectionType",
  "collectionName": "fotos",
  "info": {
    "name": "Fotos",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "nombre": {
      "type": "string",
      "required": false
    },
    "fecha_captura": {
      "type": "datetime",
      "required": false
    },
    "carpeta": {
      "model": "carpetas",
      "via": "fotos"
    },
    "etiquetas": {
      "collection": "etiquetas",
      "via": "fotos",
      "dominant": true
    },
    "color": {
      "model": "colores",
      "via": "fotos"
    },
    "puntuacion": {
      "type": "integer",
      "default": 0
    },
    "ruta_imagen_disco": {
      "type": "string"
    },
    "img": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
